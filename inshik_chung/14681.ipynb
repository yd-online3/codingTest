    "# Baekjoon 14681\n",
    "\n",
    "x = int(input())\n",
    "y = int(input())\n",
    "\n",
    "def locate(x, y):\n",
    "    if x > 0 and y > 0:\n",
    "        return 1\n",
    "    elif x < 0 and y > 0:\n",
    "        return 2\n",
    "    elif x <0 and y < 0:\n",
    "        return 3\n",
    "    else:\n",
    "        return 4\n",
    "\n",
    "print(locate(x, y))"
   ]
